<h2>All Orders</h2>
<table>
  <% orders.forEach(order => { %>
    <tr>
      <td><%= order.user.name %></td>
      <td>
        <ul>
          <% order.items.forEach(item => { %>
            <li><%= item.title %> x<%= item.quantity %></li>
          <% }) %>
        </ul>
      </td>
      <td><%= order.totalPrice %></td>
      <td><%= order.status %></td>
      <td><%= order.createdAt.toLocaleDateString() %></td>
    </tr>
  <% }) %>
</table>
